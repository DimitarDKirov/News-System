each article in result
    div.panel.panel-default
        div.panel-body
            img.img-responsive(src= article.imageUrl, alt="image")
            h2
                a(href="/article-details/" + article.id)
                    | #{article.title}
        div.panel-footer
            | published at 
            = new Date(article.publishedAt).toLocaleString()
            |  by 
            a(href="#")
                | #{article.source}